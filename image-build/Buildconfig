kind: BuildConfig
apiVersion: build.openshift.io/v1
metadata:
  annotations:
    app.openshift.io/vcs-ref: ''
    app.openshift.io/vcs-uri: 'https://github.com/MoOyeg/openshift-quick-networkpolicy-demo.git'
    openshift.io/generated-by: OpenShiftWebConsole
  name: ubi8-nc
  namespace: openshift
spec:
  output:
    to:
      kind: ImageStreamTag
      name: 'ubi8-nc:latest'
  resources: {}
  successfulBuildsHistoryLimit: 5
  failedBuildsHistoryLimit: 5
  strategy:
    type: Docker
    dockerStrategy:
      dockerfilePath: Dockerfile
  postCommit: {}
  source:
    type: Git
    git:
      uri: 'https://github.com/MoOyeg/openshift-quick-networkpolicy-demo.git'
    contextDir: /
  runPolicy: Serial

